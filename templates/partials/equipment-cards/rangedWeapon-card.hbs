<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    <img class="item-img display-inline" src="{{img}}" title="{{name}}" height="15" width="15" />
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{name}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.damage}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.range.rangeClose}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.range.rangeNormal}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.range.rangeLong}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.range.rangeMax}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    {{system.ammoGroup}}
</td>
<td class="item equippable-inventory-item" data-item-id="{{_id}}">
    <a class="item-equip" data-type="text" title={{localize "cd10.sheet.equip" }}>
        <i class="far fa-{{#if system.isEquipped}}minus{{else}}plus{{/if}}-square"></i>
    </a>
</td>
